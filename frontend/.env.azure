# Azure OpenAI Configuration
REACT_APP_AZURE_OPENAI_ENDPOINT=https://<your-resource>.openai.azure.com
REACT_APP_AZURE_API_VERSION=2023-05-15

# Azure Cognitive Search Configuration
REACT_APP_AZURE_SEARCH_ENDPOINT=https://<your-resource>.search.windows.net
REACT_APP_AZURE_SEARCH_INDEX=documents

# Azure Blob Storage Configuration
REACT_APP_AZURE_STORAGE_ENDPOINT=https://<your-resource>.blob.core.windows.net
REACT_APP_AZURE_STORAGE_CONTAINER=documents

# Feature Flags
REACT_APP_AZURE_ENABLED=true
REACT_APP_USE_AZURE_STORAGE=true
REACT_APP_USE_AZURE_OPENAI=true
REACT_APP_USE_AZURE_SEARCH=true

# API Configuration
REACT_APP_API_URL=/api